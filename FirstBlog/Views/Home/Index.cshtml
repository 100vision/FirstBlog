@model IEnumerable<FirstBlog.Models.Journal>

@{
    ViewBag.Title = "Home Page";
}

<div id="content" class="row-fluid">

    <div class="col-md-2">
        <div class="panel">
            <div class="panel-heading "><h2>Site Map</h2></div>
            <div class="panel-body"><p>hello world</p></div>
        </div>
        </div>
    </div>
    <div class="col-md-8">
        @foreach (var item in Model)
        {
            <div class="panel page-header"><h1>@item.Caption</h1></div>
                    <div class="panel-body">@item.Body</div>
                    <div class="panel-footer">@item.Date</div>
        }


    </div>
    <div class="col-md-2">
        <h2>Sidebar</h2>
    </div>
</div>








